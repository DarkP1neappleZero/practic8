print("Игра 'Угадай число'")
secret = int(input("Игрок 1, загадайте число от 1 до 10: "))

for i in range(1, 4):
    guess = int(input(f"Игрок 2, попытка {i}: "))
    if guess == secret:
        print("Угадали!")
        break
    elif guess < secret:
        print("Неверно, загаданное число больше")
    else:
        print("Неверно, загаданное число меньше")

if guess != secret:
    print(f"Загаданное число: {secret}")
